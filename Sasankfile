pipeline {
  agent any
stages{
  stage('git checkout'){
    steps{
   git branch: 'main', url: 'https://github.com/AkshayDivakaran/InsureMe-20March.git'
}
}
stage('maven package'){
  steps{
 sh 'mvn package'
}
}
stage('building image'){
 steps{
sh 'docker build -t sidharth1997/project:v1.0'
}

}

}
}


